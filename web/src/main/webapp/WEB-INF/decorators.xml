<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/40*.jsp</pattern>
        <pattern>/*ajax=true*</pattern>
        <pattern>/struts/*</pattern>
        <pattern>/resources/*</pattern>
    </excludes> 
    <decorator name="admin" page="default.jsp">
        <pattern>/admin*</pattern>
    </decorator>
    <decorator name="branch" page="default.jsp">
        <pattern>/branch*</pattern>
    </decorator>  
    <decorator name="style-home" page="vstyleu-home.jsp">
        <pattern>/style-home*</pattern>
    </decorator>
    <decorator name="vstyleu" page="vstyleu-home.jsp">
        <pattern>/*</pattern>
    </decorator>
    
</decorators>
